{
  // @babel/preset-env - 根据目标浏览器自动转换ES6+语法
  // @babel/preset-react - 转换JSX语法
  // @babel/preset-typescript - 转换TypeScript语法
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react", 
    "@babel/preset-typescript"
  ],
  "plugins": [],
  "env": {
    "development": {
      // react-refresh/babel - 开发环境启用React组件热更新
      "plugins": ["react-refresh/babel"]
    }
  }
}
